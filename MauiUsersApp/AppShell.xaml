<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiUsersApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiUsersApp"
    xmlns:views="clr-namespace:MauiUsersApp.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="MauiUsersApp">

    <ShellContent Route="LoginPage" ContentTemplate="{DataTemplate views:LoginPage}" />
    <ShellContent Route="UsersPage" ContentTemplate="{DataTemplate views:UsersPage}" />
    <ShellContent Route="EditUserPage" ContentTemplate="{DataTemplate views:EditUserPage}" />

</Shell>